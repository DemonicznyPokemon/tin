{
    "name": "loophp/tin",
    "type": "library",
    "description": "Provide validation tools for Taxpayer Identification Number",
    "homepage": "https://github.com/loophp/tin",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Portelange",
            "email": "thomas@lekoala.be"
        },
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com"
        }
    ],
    "require": {
        "php": ">= 7.1.3"
    },
    "require-dev": {
        "drupol/php-conventions": "^1.7.2 || ^1.8.6 || ^2.0.2",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "infection/infection": "^0.13.6 || ^0.15.3",
        "phpspec/phpspec": "^5.1.2 || ^6.3",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "symfony/polyfill-mbstring": "^1.18",
        "vimeo/psalm": "^3.18 || ^4"
    },
    "autoload": {
        "psr-4": {
            "loophp\\Tin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\loophp\\Tin\\": "tests/src/"
        }
    },
    "support": {
        "issues": "https://github.com/loophp/tin/issues",
        "source": "https://github.com/loophp/tin"
    }
}
